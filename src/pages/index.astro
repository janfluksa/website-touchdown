---
import Navbar from '../components/Navbar.astro';
import Layout from '../components/@Layout.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';

import Introduction from '../components/Introduction.astro';
import Services from '../components/Services.astro';
import Sectors from '../components/Sectors.astro';
import Team from '../components/Team.astro';
import SigmaSearch from '../components/SigmaSearch.astro';
import Partners from '../components/Partners.astro'
import Media from '../components/Media.astro'
import Contact from '../components/Contact.astro'
import Footer from '../components/Footer.astro';
import LatestMedia from '../components/_latestMedia.astro';
---


<Layout title="Touchdown" description={SITE_DESCRIPTION}>
	
		<Navbar />

		<Introduction />

		<main>
			<Services />
			<Sectors />
			<Team />
			<SigmaSearch />

			<Media />
			<Partners />
			<Contact />
			
			<Footer />
		</main>

		
</Layout>